/* -*- c -*- */

header {
##include "los/ltuple.h"
##include "los/lint.h"
}

data { LTuple *tuple; }

setup
{
  unsigned int i;
  data->tuple = l_tuple_new(4);
  for (i = 0; i < 4; i++)
    l_tuple_give_item(data->tuple, i, L_OBJECT(l_int_new(i)));
}

teardown { L_OBJECT_CLEAR(data->tuple); }

test
{ 
  PIF_CHECK(data->tuple);
}

